#!/bin/bash

file_name=$1


while IFS="\n" read -r line
do
  key=$(echo "$line" | cut -d '=' -f 1)
  value=$(echo "$line" | cut -d '=' -f 2)

  #Apply these condition on two spaces
  sed -i "s/<< $key >>/$value/g" /tmp/tmp.yaml

  #Apply these condition on no spaces
  sed -i "s/<<$key>>/$value/g" /tmp/tmp.yaml

done < $file_name
